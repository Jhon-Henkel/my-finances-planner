{
    "name": "laravel/my_finances_planner",
    "type": "project",
    "description": "App for finances planner",
    "version": "2.3.0",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "cronitor/cronitor-php": "^1.3",
        "firebase/php-jwt": "^6.8",
        "guzzlehttp/guzzle": "^7.2",
        "honeybadger-io/honeybadger-laravel": "^4.1.0",
        "laravel/framework": "^11.18",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^11.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "test:unit": "vendor/bin/phpunit ./tests/backend/Unit",
        "test:feature": "vendor/bin/phpunit ./tests/backend/Feature",
        "test:coverage": "vendor/bin/phpunit --coverage-html ./tests/backend/coverage/",
        "phpstan": "./vendor/bin/phpstan analyse --level=5 --memory-limit=512M app",
        "pint": "./vendor/bin/pint --test --preset psr12",
        "pint-correct": "./vendor/bin/pint --preset psr12"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
